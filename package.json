  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "ali-oss": "^6.16.0",
    "bluebird": "^3.7.2",
    "egg-freelog-base": "^3.1.28",
    "egg-scripts": "^2.15.1",
    "file-type": "^16.5.4",
    "identicon.js": "^2.3.3",
    "jsonschema": "^1.4.0",
    "midway": "^1.20.3",
    "moment": "^2.29.1",
    "mongoose": "^5.13.9",
    "nodemailer": "^5.1.1",
    "patrun": "^4.0.0",
    "probe-image-size": "^5.0.0",
    "svg-captcha": "^1.4.0",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "15.6.1",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.19.0",
    "midway-bin": "^1.20.3",
    "midway-mock": "^1.20.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-midway-contrib": "^1.20.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=freelog-node-provider --workers=1 --framework=midway --ts",
    "stop": "egg-scripts stop --title=freelog-node-provider",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local port=7111 midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "autod": "midway-bin autod"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
